{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "src/template/component/TemplateName.tsx",
        "style": "src/template/component/TemplateName.css"
      },
      "path": "src/components",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "src/template/component/TemplateName.tsx",
        "style": "src/template/component/TemplateName.css"
      },
      "path": "src/pages",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}
